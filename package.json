{
  "name": "goosejob-web-nextjs-cloudflare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "b": "bun run build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --incremental",
    "s": "shadcn",
    "docker:up": "docker compose up -d",
    "docker:up:prod": "docker compose -f docker-compose.prod.yaml up -d",
    "db": "prisma",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:gen": "prisma generate --no-hints",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "deploy": "opennextjs-cloudflare && wrangler deploy",
    "preview": "opennextjs-cloudflare && wrangler dev",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg-worker": "^6.2.1",
    "@prisma/client": "^6.2.1",
    "@prisma/pg-worker": "^6.2.1",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.471.1",
    "next": "^15.1.4",
    "next-themes": "^0.4.4",
    "react": "^18",
    "react-dom": "^18",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@opennextjs/cloudflare": "0.3.x",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.23",
    "postcss": "^8",
    "prisma": "^6.2.1",
    "shadcn": "^2.1.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^3.102.0"
  }
}
