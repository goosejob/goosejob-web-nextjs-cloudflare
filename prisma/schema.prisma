generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id String @id @default(ulid())

  slug        String  @unique
  title       String
  description String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
